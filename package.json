{
  "name": "ugrid",
  "version": "0.0.0",
  "description": "ugrid client and server",
  "main": "index.js",
  "scripts": {
    "start": "cd bin; ./ugrid.js -w 12348 & echo $! >/tmp/ugrid.pid ; sleep 1",
    "pretest": "echo 'export UGRID_WORKER_PER_HOST=1' > conf/ugrid-env.sh; ./bin/start-all.sh",
    "test": "cd test; ./runUnitTest.js",
    "posttest": "bin/stop-all.sh",
    "stop": "bin/stop-all.sh"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@gitlab.ugrid.net:10022/ugrid/ugrid.git"
  },
  "keywords": [
    "grid",
    "parallel",
    "cluster",
    "hpc"
  ],
  "author": "Luca SAS",
  "dependencies": {
    "body-parser": "^1.9.2",
    "co": "^3.1.0",
    "express": "^4.10.1",
    "node-getopt": "^0.2.3",
    "node-uuid": "^1.4.1",
    "rand-token": "^0.2.1",
    "request": "^2.49.0",
    "thunkify": "^2.1.2",
    "websocket-stream": "^1.1.1",
    "ws": "^0.4.32",
    "ssh2": "^0.3.6"
  },
  "jshintConfig": {
    "node": true,
    "esnext": true,
    "undef": true,
    "unused": true,
    "loopfunc": true
  }
}
