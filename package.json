{
  "name": "ugrid",
  "version": "0.2.0",
  "license": "MIT",
  "description": "ugrid client and server",
  "main": "index.js",
  "scripts": {
    "preinstall": "echo 'module.exports = require(\"..\")' > node_modules/ugrid.js",
    "start": "bin/start-all.sh",
    "stop": "bin/stop-all.sh",
    "test": "node_modules/.bin/mocha"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@gitlab.ugrid.net:10022/ugrid/ugrid.git"
  },
  "publishConfig": {
    "registry": "http://gitlab.ugrid.net:4873/"
  },
  "keywords": [
    "grid",
    "parallel",
    "cluster",
    "hpc"
  ],
  "author": "Luca SAS",
  "dependencies": {
    "body-parser": "^1.14.0",
    "connect-busboy": "0.0.2",
    "express": "^4.13.3",
    "kerberos": "0.0.17",
    "line-trace": "^1.0.4",
    "lodash": "^3.10.1",
    "mongodb": "^2.0.33",
    "morgan": "^1.6.1",
    "node-getopt": "^0.2.3",
    "node-uuid": "^1.4.1",
    "pg": "^4.4.1",
    "ssh2": "^0.4.4",
    "stream-to-array": "^2.1.0",
    "thenify": "^3.1.0",
    "websocket-stream": "^2.0.2",
    "ws": "^0.8.0"
  },
  "devDependencies": {
    "mocha": "^2.2.5"
  },
  "jshintConfig": {
    "node": true,
    "esnext": true,
    "undef": true,
    "unused": true,
    "loopfunc": true
  },
  "eslintConfig": {
    "rules": {
        "indent": [
            2,
            "tab"
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ]
    },
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended"
  }
}
